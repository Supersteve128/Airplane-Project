					"actions": [
						{
							"id": "push",
							"objectClass": "LogMessages",
							"sid": 727919727258743,
							"parameters": {
								"where": "back",
								"value": "message",
								"axis": "x"
							}
						}
					],
					"sid": 339498089273905,
					"children": [
						{
							"eventType": "comment",
							"text": "If we are over 20 messages, delete the first (oldest) message."
